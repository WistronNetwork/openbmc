+++ b/src/PSUSensorMain.cpp
@@ -75,6 +75,7 @@ static constexpr auto sensorTypes{std::to_array<const char*>(
      "xyz.openbmc_project.Configuration.RAA229004",
      "xyz.openbmc_project.Configuration.RAA229126",
      "xyz.openbmc_project.Configuration.TPS546D24",
+     "xyz.openbmc_project.Configuration.UCD90160",
      "xyz.openbmc_project.Configuration.XDPE12284"})};
 
 // clang-format off
@@ -113,6 +114,7 @@ static constexpr auto pmbusNames{std::to_array<const char*>({
     "raa229004",
     "raa229126",
     "tps546d24",
+    "ucd90160",
     "xdpe12284"
 })};
 //clang-format on
