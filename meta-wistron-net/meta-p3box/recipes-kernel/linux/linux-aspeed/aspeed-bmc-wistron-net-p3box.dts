		i2c43 = &imux43;
		i2c44 = &imux44;
		i2c45 = &imux45;
		i2c46 = &imux46;
		i2c47 = &imux47;
		i2c48 = &imux48;
		i2c49 = &imux49;
		i2c50 = &imux50;
		i2c51 = &imux51;
		i2c52 = &imux52;
		i2c53 = &imux53;
		i2c54 = &imux54;
		i2c55 = &imux55;
		i2c56 = &imux56;
		i2c57 = &imux57;
		i2c58 = &imux58;
		i2c59 = &imux59;
		i2c60 = &imux60;
		i2c61 = &imux61;
		i2c62 = &imux62;
		i2c63 = &imux63;

		spi0 = &fmc;
	};

	chosen {
		stdout-path = &uart5;
		bootargs = "console=ttyS4,115200 root=/dev/ram rw mem.devmem=1";
	};

	memory@80000000 {
		reg = <0x80000000 0x20000000>;
	};

	iio-hwmon {
		compatible = "iio-hwmon";
		io-channels = <&adc 0>, <&adc 1>, <&adc 2>, <&adc 3>, <&adc 4>, <&adc 5>,
		              <&adc 6>, <&adc 8>, <&adc 9>;
	};
};

&fmc {
	status = "okay";
	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "bmc";
		spi-max-frequency = <50000000>;
#include "openbmc-flash-layout-128.dtsi"
	};

	flash@1 {
		status = "okay";
		m25p,fast-read;
		label = "alt-bmc";
		spi-max-frequency = <50000000>;
#include "openbmc-flash-layout-128-alt.dtsi"
	};
};

&uart5 {
	status = "okay";
};

&mac0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_rgmii1_default &pinctrl_mdio1_default>;
	phy-mode = "rgmii";
	phy-handle = <&phy>;

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		phy: ethernet-phy@1 {
			/* RTL8211F */
			compatible = "ethernet-phy-id001c.c916";
			reg = <1>;

			realtek,reg-init = <0xd04 0x10 0x0 0x17b>;
		};
	};
};

&i2c0 {
	status = "okay";
	bus-frequency = <400000>;

	i2c-mux@70 {
		compatible = "nxp,pca9546";
		#address-cells = <1>;
		#size-cells = <0>;
		i2c-mux-idle-disconnect;
		reg = <0x70>;

		imux32: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			optoe@50 {
				compatible = "optoe,optoe3";
				reg = <0x50>;
				port_name = "Port1";
			};
		};

		imux33: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;

			optoe@50 {
				compatible = "optoe,optoe3";
				reg = <0x50>;
				port_name = "Port2";
			};
		};

		imux34: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;

			optoe@50 {
				compatible = "optoe,optoe3";
				reg = <0x50>;
				port_name = "Port3";
			};
		};

		imux35: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;

			optoe@50 {
				compatible = "optoe,optoe3";
				reg = <0x50>;
				port_name = "Port4";
			};
		};
	};
};

