&i2c3 {
	status = "okay";

	system_cpld1: cpld@11 {
		compatible = "wistron,system_cpld1";
		reg = <0x11>;
	};

	system_cpld2: cpld@12 {
		compatible = "wistron,system_cpld2";
		reg = <0x12>;
	};

	fan_cpld: cpld@16 {
		compatible = "wistron,fan_cpld";
		reg = <0x16>;
	};

	i2c-mux@70 {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		i2c-mux-idle-disconnect;
		reg = <0x70>;

		imux16: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			/* MB Temperature1 */
			tmp75@48 {
				compatible = "ti,tmp75";
				reg = <0x48>;
			};

			/* MB Temperature2 */
			tmp75@49 {
				compatible = "ti,tmp75";
				reg = <0x49>;
			};

			/* MB Temperature3 */
			tmp75@4a {
				compatible = "ti,tmp75";
				reg = <0x4a>;
			};

			/* MB Temperature4 */
			tmp75@4b {
				compatible = "ti,tmp75";
				reg = <0x4b>;
			};
		};

		imux17: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;

			/* PSU1 MCU */
			psu1@58 {
				compatible = "pmbus_psu";
				reg = <0x58>;
			};

			/* PSU1 EEPROM */
			psu1_eeprom@50 {
				compatible = "atmel,24c02";
				reg = <0x50>;
			};

			/* PSU2 MCU */
			psu2@59 {
				compatible = "pmbus_psu";
				reg = <0x59>;
			};

			/* PSU2 EEPROM */
			psu2_eeprom@51 {
				compatible = "atmel,24c02";
				reg = <0x51>;
			};
		};

		imux18: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;

			/* MB EEPROM */
			eeprom@50 {
				compatible = "atmel,24c02";
				reg = <0x50>;
			};
		};

		imux19: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;

			tps53688@62 {
				compatible = "ti,tps53688";
				reg = <0x62>;
			};
		};

		imux20: i2c@4 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <4>;
		};

		imux21: i2c@5 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <5>;
		};

		imux22: i2c@6 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <6>;
		};

		imux23: i2c@7 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <7>;
		};
	};

	i2c-mux@73 {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		i2c-mux-idle-disconnect;
		reg = <0x73>;

		imux24: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			/* FAN1 EEPROM */
			fan1_eeprom@51 {
				compatible = "atmel,24c02";
				reg = <0x51>;
			};

			/* FAN2 EEPROM */
			fan2_eeprom@53 {
				compatible = "atmel,24c02";
				reg = <0x53>;
			};

			/* FAN3 EEPROM */
			fan3_eeprom@55 {
				compatible = "atmel,24c02";
				reg = <0x55>;
			};
		};

		imux25: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};

		imux26: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;

			/* FAN4 EEPROM */
			fan4_eeprom@51 {
				compatible = "atmel,24c02";
				reg = <0x51>;
			};

			/* FAN5 EEPROM */
			fan5_eeprom@53 {
				compatible = "atmel,24c02";
				reg = <0x53>;
			};

			/* FAN6 EEPROM */
			fan6_eeprom@55 {
				compatible = "atmel,24c02";
				reg = <0x55>;
			};
		};

		imux27: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
		};

		imux28: i2c@4 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <4>;

			/* FAN Temperature1 */
			tmp75@4e {
				compatible = "ti,tmp75";
				reg = <0x4e>;
			};

			/* FAN Temperature2 */
			tmp75@4f {
				compatible = "ti,tmp75";
				reg = <0x4f>;
			};
		};

		imux29: i2c@5 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <5>;
		};

		imux30: i2c@6 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <6>;
		};

		imux31: i2c@7 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <7>;
		};
	};
};

