From 52d0c8f7aee5100d307adaa2946f666ac3a03c9c Mon Sep 17 00:00:00 2001
From: Neal Chen <neal_chen@wistron.com>
Date: Tue, 7 Jun 2022 14:45:50 +0800
Subject: [PATCH] hwmon: wistron: Add support wistron CPLD

Signed-off-by: Neal Chen <neal_chen@wistron.com>
---
 drivers/hwmon/Kconfig  | 2 ++
 drivers/hwmon/Makefile | 1 +
 2 files changed, 3 insertions(+)

diff --git a/drivers/hwmon/Kconfig b/drivers/hwmon/Kconfig
index 81e7d40e76d2..ab444fea5cdd 100644
--- a/drivers/hwmon/Kconfig
+++ b/drivers/hwmon/Kconfig
@@ -2292,4 +2292,6 @@ config SENSORS_ATK0110
 
 endif # ACPI
 
+source "drivers/hwmon/wistron/Kconfig"
+
 endif # HWMON
diff --git a/drivers/hwmon/Makefile b/drivers/hwmon/Makefile
index a5be35143af5..081cd5869260 100644
--- a/drivers/hwmon/Makefile
+++ b/drivers/hwmon/Makefile
@@ -211,6 +211,7 @@ obj-$(CONFIG_SENSORS_XGENE)	+= xgene-hwmon.o
 
 obj-$(CONFIG_SENSORS_OCC)	+= occ/
 obj-$(CONFIG_PMBUS)		+= pmbus/
+obj-$(CONFIG_WISTRON)		+= wistron/
 
 ccflags-$(CONFIG_HWMON_DEBUG_CHIP) := -DDEBUG
 
